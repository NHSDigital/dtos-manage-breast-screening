{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "module": "Preserve",
    "moduleResolution": "Node",
    "noEmit": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": false,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true
  },
  "exclude": ["**/node_modules"],
  "typeAcquisition": {
    "enable": false
  }
}
